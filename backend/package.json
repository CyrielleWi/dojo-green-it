{
  "name": "project",
  "version": "0.0.0",
  "description": "description",
  "author": "AleBlondin",
  "license": "MIT",
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "premigration:generate": "nest build -p tsconfig.json",
    "migration:generate": "typeorm migration:generate",
    "migration:revert": "typeorm migration:revert",
    "premigration:run": "nest build -p tsconfig.json",
    "migration:run": "typeorm migration:run",
    "test:migration:run": "ts-node -r tsconfig-paths/register -r ./src/dotenv/config node_modules/.bin/typeorm migration:run dotenv_config_path=.env.test",
    "schema:drop": "typeorm schema:drop",
    "test:schema:drop": "ts-node -r ./src/dotenv/config node_modules/.bin/typeorm schema:drop dotenv_config_path=.env.test",
    "reset": "rm -rf node_modules && yarn install",
    "start": "nest start -p ./tsconfig.json src/main.ts",
    "start:dev": "yarn start --watch",
    "start:debug": "yarn start:dev --debug",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint './**/*.{js,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "pretest": "yarn test:migration:run",
    "test": "ts-node -r ./src/dotenv/config node_modules/.bin/jest * dotenv_config_path=.env.test --maxWorkers=1",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:debug": "NODE_OPTIONS='--inspect-brk' yarn test",
    "test:e2e": "ts-node -r ./src/dotenv/config node_modules/.bin/jest --config ./test/jest-e2e.json * dotenv_config_path=.env.test --maxWorkers=1"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.11",
    "@nestjs/core": "^7.0.11",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mapped-types": "^0.1.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.11",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@types/cls-hooked": "^4.3.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cli-color": "^2.0.0",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.11",
    "logform": "^2.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.18",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/testing": "^7.0.11",
    "@types/bcrypt": "^3.0.0",
    "@types/cli-color": "^2.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv-safe": "^5.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.121",
    "@types/node": "^10.7.1",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^4.4.2",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^helpers/(.*)": "<rootDir>/helpers/$1",
      "^auth/(.*)": "<rootDir>/modules/auth/$1"
    },
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
